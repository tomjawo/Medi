@model List<Medi.Models.Examination>

@{
    ViewBag.Title = "Lista zleconych badań";
}

<h2>Lista badań:</h2>
 
@if (Model.Count() == 0)
{
    <h3> @ViewBag.Message </h3>
}

@foreach(var exam in Model)
{
    @exam.ExaminationTape
    
    <br>
    @exam.OrderedBy.FirstName @exam.OrderedBy.LastName
}



